<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.montyskew.momentum" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        >
        <activity
            android:name="com.montyskew.momentum.Home"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.montyskew.momentum.Activities"
            android:label="@string/activities">
        </activity>
        <activity
            android:name="com.montyskew.momentum.Goal"
            android:label="@string/goal">
        </activity>
        <activity
            android:name="com.montyskew.momentum.Users"
            android:label="@string/users">
        </activity>
    </application>

</manifest>
